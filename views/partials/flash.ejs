<% if(success != '') { %>
    <div class="alert alert-success alert-dismissible border-dark fade show shadow-lg">
        <%= success %>
        <button type="button" class="close" data-dismiss="alert">&times;</button>
    </div>
<% } %>

<% if(error != '') { %>
    <div class="alert alert-danger alert-dismissible border-dark fade show shadow-lg">
        <%= error %>
        <button type="button" class="close" data-dismiss="alert">&times;</button>
    </div>
<% } %>